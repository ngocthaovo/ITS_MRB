@{
    ViewBag.Title = "SAPendingList";
    Layout = "~/Views/Shared/_FEAContentLayout.cshtml";
}
<div class="panel-body">
    <form id="frmMainForm">
        <div class="row buttonbar">
            <div class="col-lg-7">
                @Html.DevExpress().Button(
                settings =>
                {
                    settings.Name = "btnRefresh";
                    settings.Width = 90;
                    settings.Text = @Resources.Resource.Refresh;
                    settings.CausesValidation = false;
                    settings.ClientSideEvents.Click = "function(){window.location.reload();}";
                }).GetHtml()
            </div>
        </div>
        <!-- line-->
        <hr />
        <!--Line-->
        <h4 class="group-title"><i class="icon-edit-sign teal"></i> @Resources.Resource.ListWaitingProcession</h4>
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <div class="vertComponentContainer">
                        @{Html.RenderAction("GetSAPendingListPartial", "SA");}
                    </div>
                </div>
            </div>
        </div>

    </form>
    </div>
